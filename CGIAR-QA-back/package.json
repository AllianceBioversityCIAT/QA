{
   "name": "CGIAR-QA-back",
   "version": "0.0.1",
   "description": "CGIAR QA backend app",
   "devDependencies": {
      "@types/node": "^8.0.29",
      "ts-loader": "^6.2.1",
      "ts-node": "^8.6.2",
      "typescript": "^3.8.3",
      "webpack": "^4.42.0",
      "webpack-cli": "^3.3.11",
      "webpack-node-externals": "^1.7.2"
   },
   "dependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/body-parser": "^1.17.1",
      "@types/cors": "^2.8.6",
      "@types/helmet": "0.0.45",
      "@types/jsonwebtoken": "^8.3.7",
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.18.1",
      "class-validator": "^0.11.0",
      "cors": "^2.8.5",
      "express": "^4.15.4",
      "helmet": "^3.21.2",
      "jsonwebtoken": "^8.5.1",
      "module-alias": "^2.2.2",
      "mysql": "^2.18.1",
      "reflect-metadata": "^0.1.10",
      "sqlite3": "^4.0.3",
      "ts-node-dev": "^1.0.0-pre.44",
      "typeorm": "0.2.22"
   },
   "scripts": {
      "build": "webpack",
      "tsc": "tsc",
      "start": "set debug=* && npx tsnd --respawn --transpileOnly ./src/index.ts",
      "prod": "tsc && node ./build/app.js",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run"
